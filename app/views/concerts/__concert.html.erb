  <p>
    <strong>Concerts artiste:</strong>
    <%= _concert._concerts_artiste_id %>
  </p>

  <p>
    <strong>Nom:</strong>
    <%= _concert.nom %>
  </p>

  <p>
    <strong>Endroit:</strong>
    <%= _concert.endroit %>
